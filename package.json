        "debuggers": [
            {
                "type": "apim-policy",
                "label": "Attach to APIM",
                "runtime": "node",
                "configurationAttributes": {
                    "launch": {
                        "properties": {
                            "stopOnEntry": {
                                "type": "boolean",
                                "description": "Automatically stop after launch.",
                                "default": true
                            }
                        }
                    }
                },
                "initialConfigurations": [
                    {
                        "type": "apim-policy",
                        "request": "launch",
                        "name": "Attach to APIM",
                        "stopOnEntry": true
                    }
                ]
            }
        ],
