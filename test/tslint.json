{
    "extends": "../tslint.json",
    "rules": {
        "no-console": false,
        "no-implicit-dependencies": [
            true,
            "dev"
        ],
        "typedef": [
            true,
            "call-signature",
            // "arrow-call-signature",
            "parameter",
            // "arrow-parameter",
            "property-declaration",
            "variable-declaration",
            "member-variable-declaration"
            // "object-destructuring",
            // "array-destructuring"
        ],
        "no-unexternalized-strings": false
    }
}
