    <xs:complexType name="AuthenticateTokenConfiguration">
        <xs:attribute name="id" type="xs:string" use="optional" />
        <xs:attribute name="token-id" type="xs:string" />
        <xs:attribute name="client-id" type="xs:string" use="optional" />
